server {
  listen              80;
  server_name					mvp1.thisis.me;
  access_log          on;
  error_log           /var/log/nginx/tim-mobile.error.log;

  charset             utf-8;

  location / {
      uwsgi_pass      unix:///tmp/tim-mobile.sock;
      auth_basic      "thisis.me";
      auth_basic_user_file sites-available/htpasswd;
  }
}
