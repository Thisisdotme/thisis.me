server {
  listen              80;
  server_name					mvp2.thisis.me;
  access_log          on;
  error_log           /var/log/nginx/tim-mobile-v2.error.log;

  charset             utf-8;
  location / {
      uwsgi_pass      unix:///tmp/tim-mobile-v2.sock;
      include         uwsgi_params;
      uwsgi_param     SCRIPT_NAME /;
  }
}
