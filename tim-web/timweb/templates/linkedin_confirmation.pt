<metal:index use-macro="load: main_structure.html">
	<section metal:fill-slot="main_content">
		<h1>${message} Below is a little information from LinkedIn</h1>
		<div>
			Firstname: ${firstname}<br/>
			Lastname: ${lastname}<br/>
			Headline: ${headline}<br/>
			<a href="${profileURL}">Your Profile</a><br/>
			First degree connections: ${firstdegree}<br/>
			Second degree connections: ${seconddegree}<br/>
			<h2>Your recent updates</h2>
			${structure:updates}
			<h2>Your connections</h2>
			<ul>${structure:connections}<ul>
		</div>
	</section>
</metal:index>
