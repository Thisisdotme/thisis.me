<div class="bio">
  <div id="bio-wrapper">
  	<h1>{name}</h1>
  	<div class="headline">{headline}</div>
  	<div class="profile-img">
  	  <img src="{picture_url}"/>
  	</div>
  	<div class="location"><strong>Location: </strong>{location}</div>
  	{?specialties}
  	  <div class="specialties"><strong>Specializing in: </strong>{specialties}</div>
  	{/specialties}
  	{?summary}
  	  <div class="summary"><strong>Summary: </strong>{summary}</div>
  	{/summary}
	</div>
</div>