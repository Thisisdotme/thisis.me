<div class="page flip-{pageNum}" data-num="{pageNum}" style="z-index:{zIndex}">
  <div class="front">
  	<div class="outer">
  		<div class="content" style="">
  			<div class="inner">{frontContent|s}</div>
  		</div>
  	</div>
  </div>
  <div class="back">
  	<div class="outer">
  		<div class="content" style="">
  			<div class="inner">{backContent|s}</div>
  		</div>
  	</div>
  </div>
</div>